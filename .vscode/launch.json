{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Debug Jest Lighthouse Tests",
      "type": "node",
      "request": "launch",
      "env":{"LIGHTHOUSE_URL":"https://staging.nathanvale.dev/","CIRCLE_SHELL_ENV":"local"},
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--config",
        "./lighthouse/jest.config.js",
        "--verbose",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    }
  ]
}